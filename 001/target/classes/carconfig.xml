<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean name="cars" class="com.maven.springer.referencing.CarDealer">
        <property name="name" value="Super Dealers"/>
        <property name="models">
            <list>
                <value>Subaru</value>
                <value>Toyota</value>
                <value>VW</value>
                <value>BMW</value>
                <value>Mercedes</value>
                <value>Volvo</value>
                <value>Mistubishi</value>
                <value>Honda</value>
                <value>Hyundai</value>
                <value>Range Rover</value>
                <value>Swift</value>
                <value>Mazda</value>
            </list>
        </property>
    </bean>
</beans>